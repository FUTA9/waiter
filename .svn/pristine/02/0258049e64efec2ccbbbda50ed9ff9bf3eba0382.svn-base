$(function(){
	/**
	 * 获取时间
	 * [mydate description]
	 * @type {Date}
	 */
	var mydate = new Date();
	var time=mydate.toLocaleString();
	$('.time span').text(time);

	/**
	 * 监听回车事件
	 * [monitor description]
	 * @type {[type]}
	 */
	var monitor = $('#send');
	monitor.keypress(function(e){
		var key = e.which;
		if (key == 13) { 
			alert("aaa"); 
		} 
	}); 


});
